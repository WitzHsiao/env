# rbenv for zsh
if which rbenv > /dev/null; then eval "$(rbenv init - zsh)"; fi


export PATH=~/Library/Android/sdk/platform-tools/:$PATH
export PATH=~/.go/bin:$PATH
export GOPATH=~/.go

# aliases
#alias vim='nvim'
alias tig='tig --all'

# install antigen
source "$HOME/.antigen/antigen.zsh"
antigen-apply
